{
    "name": "b2pweb/bdf-prime-bundle",
    "type": "symfony-bundle",
    "description": "Symfony PrimeBundle",
    "keywords": ["orm", "active record", "data mapper"],
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ©bastien Tanneux"
        },
        {
            "name": "Vincent Quatrevieux"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1",
        "b2pweb/bdf-prime": "~1.2",
        "b2pweb/bdf-prime-persistence": "~1.1",
        "symfony/config": "^4.0|^5.0",
        "symfony/dependency-injection": "^4.0|^5.0",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/service-contracts": "^1.1|^2.0",
        "doctrine/doctrine-bundle": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0",
        "symfony/yaml": "~3.4 || ~4.0 || ^5.0",
        "symfony/console": "~3.4 || ~4.0 || ^5.0",
        "symfony/web-profiler-bundle": "~3.4 || ~4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": { "Bdf\\PrimeBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "tests": "phpunit",
        "tests-with-coverage": "phpunit --coverage-clover coverage.xml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "suggest": {
        "symfony/web-profiler-bundle": "To use the data collector."
    }
}
